# 应用配置
NODE_ENV=development
PORT=4000
API_PREFIX=/api

# MongoDB配置
MONGODB_URI=mongodb://localhost:27017/jin33
MONGODB_USER=
MONGODB_PASS=

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT配置
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=7d

# 日志配置
LOG_LEVEL=info
LOG_DIR=logs

# 安全配置
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Socket.IO配置
SOCKET_PATH=/socket.io
SOCKET_CORS_ORIGIN=http://localhost:3000
